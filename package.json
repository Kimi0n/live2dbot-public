{
  "name": "live2dbot",
  "version": "0.4.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start-server": "ng build && copy /Y server.js dist\\live2dbot\\ && mkdir dist\\live2dbot\\server && copy /Y server\\*.* dist\\live2dbot\\server && node dist\\live2dbot\\server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "make": "ng build && copy /Y server.js dist\\live2dbot\\ && nexe dist\\live2dbot\\server.js -r \"dist/live2dbot/*.*\" --build -o dist\\live2dbot_0.4.3beta.exe"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "fs": "^0.0.1-security",
    "nexe": "^4.0.0-rc.1",
    "open": "^8.4.0",
    "rxjs": "~7.5.0",
    "tmi.js": "^1.8.5",
    "tslib": "^2.3.0",
    "twitch-bot": "^1.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular/cli": "~14.1.2",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.7.6",
    "@types/tmi.js": "^1.8.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
